set terminal postscript enhanced color 20
set output 'threading.eps'
set bar 1.000000
set style rectangle back fc lt -3 fillstyle solid 1.00 border -1
set key inside left top vertical Left noreverse enhanced autotitles columnhead nobo
set datafile missing '-'
#set data style linespoints 
set style data linespoints# linewidth 5
set xtics border in scale 1,0.5 nomirror offset character 0, 0, 0
set rrange [ * : * ] noreverse nowriteback  # (currently [0.00000:10.0000] )
set trange [ * : * ] noreverse nowriteback  # (currently [-5.00000:5.00000] )
set urange [ * : * ] noreverse nowriteback  # (currently [-5.00000:5.00000] )
set vrange [ * : * ] noreverse nowriteback  # (currently [-5.00000:5.00000] )
set ylabel  offset character 0, 0, 0 font "" textcolor lt -1 rotate by 90
set ylabel "Percentage improvement" font "Times-Roman,30"
set xlabel "Number of threads" font "Times-Roman,30"
set y2label  offset character 0, 0, 0 font "" textcolor lt -1 
set cblabel  offset character 0, 0, 0 font "" textcolor lt -1
set locale "C"
set  pointsize 2
plot 'sspace_threading.dat' using 2:xtic(1) lw 2 pt 14 lc 'black', \
'' u 3 lw 2 lt 1 lc rgb 'dark-turquoise' pt 8, \
'' u 4 lw 2 lt 1 lc rgb 'dark-violet' pt 6, \
'' u 5 lw 2 lt 1 lc rgb 'red' pt 2, \
'' u 6 lw 2 lt 1 lc rgb 'dark-green' pt 10, \
'' u 7 lw 2 lt 1 lc rgb 'dark-goldenrod' pt 4, \
'' u 8 lw 2 lt 1 lc rgb 'blue' pt 12
